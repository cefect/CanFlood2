    border: 2px solid #4E6F6D;          /* a shade lighter than the fill */
    border-radius: 4px;                 /* gentle rounding */
    padding: 4px 8px;                   /* keeps text from kissing the frame */
}

QPushButton:pressed {
    background-color: #1E2F2F;                    /* darker shade */
    border: 2px inset #3A5554;                    /* subtle inset look */
    padding-top: 5px; padding-left: 11px;         /* shift content down/right slightly */
    padding-bottom: 3px; padding-right: 9px;      /* keep overall size the same */
}

QPushButton:disabled {
    background-color: rgba(43, 64, 63, 0.50);   /* same hue, 50 % opacity  */
    color: rgba(255, 255, 255, 0.60);           /* soft-faded text          */
    border: 2px solid rgba(78, 111, 109, 0.50); /* outline still visible    */
}

 
